<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="@style/Theme.AppCompat"></style>
    <style name="phone">
        <item name="android:background">#cdf1e3</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="Label">
        <item name="android:textColor">#56ac73</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_marginTop">80dp</item>
    </style>

    <style name="Button">
        <item name="android:textStyle">italic</item>
        <item name="android:background">@drawable/button_sel</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
        <item name="android:translationX">3dp</item>
        <item name="android:translationY">3dp</item>
    </style>

    <style name="Button_serv2">
        <item name="android:textStyle">italic</item>
        <item name="android:background">@drawable/button_sel</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:layout_marginLeft">30dp</item>
    </style>

    <style name="Button_serv2.playStop">
        <item name="android:shape">oval</item>
        <item name="android:innerRadius">10dp</item>
        <item name="android:topLeftRadius">10dp</item>
        <item name="android:bottomLeftRadius">10dp</item>
        <item name="android:bottomRightRadius">10dp</item>
        <item name="android:topRightRadius">10dp</item>
    </style>

    <style name="prBar" parent="@style/Widget.AppCompat.SeekBar">
        <!--<item name="android:clipOrientation">vertical</item>-->
        <!--<item name="android:layout_height">match_parent</item>-->
    </style>


</resources>